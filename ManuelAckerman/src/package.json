{
  "name": "manuel-ackerman-web",
  "version": "1.0.0",
  "description": "Landing page moderna para Manuel Ackerman",
  "author": "Manuel Ackerman",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/manuel-ackerman-web/stats.json",
    "serve:prod": "ng build --configuration production && npx http-server dist/manuel-ackerman-web",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
    "deploy:ghpages": "ng deploy --base-href=/manuel-ackerman-web/",
    "prebuild": "npm run lint",
    "postbuild": "echo 'Build completed successfully!'",
    "clean": "rimraf dist node_modules/.cache",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.0",
    "@angular-eslint/eslint-plugin": "^17.0.0",
    "@angular-eslint/eslint-plugin-template": "^17.0.0",
    "@angular-eslint/schematics": "^17.0.0",
    "@angular-eslint/template-parser": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "angular-cli-ghpages": "^1.0.0",
    "eslint": "^8.42.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-headless": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "~5.2.0",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "lint-staged": {
    "*.{ts,html,scss,json}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "keywords": [
    "angular",
    "landing-page",
    "typescript",
    "scss",
    "responsive",
    "modern"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/manuel-ackerman-web.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/manuel-ackerman-web/issues"
  },
  "homepage": "https://github.com/tu-usuario/manuel-ackerman-web#readme"
}